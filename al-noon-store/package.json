{
  "name": "al-noon-store",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "prebuild": "node scripts/generate-api-types.mjs --silent",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr": "node dist/al-noon-store/server/server.mjs",
    "serve:ssr:al-noon-store": "node dist/al-noon-store/server/server.mjs",
    "generate:api-types": "openapi-typescript http://localhost:4000/api-docs/spec.json -o src/app/core/types/api.schema.ts",
    "generate:api-types:local": "node scripts/generate-api-types.mjs",
    "sync:api": "node scripts/generate-api-types.mjs",
    "lint": "ng lint",
    "lint:fix": "ng lint --configuration=fix",
    "test:ci": "ng test --no-watch --configuration=ci",
    "e2e": "cypress run",
    "e2e:open": "cypress open",
    "e2e:serve": "npx serve -s dist/al-noon-store/browser -l 4202",
    "e2e:ci": "npx start-server-and-test \"ng serve --configuration=e2e --port=4202\" http://localhost:4202 \"cypress run --config baseUrl=http://localhost:4202\""
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@10.8.2",
  "dependencies": {
    "@angular/common": "^21.2.0",
    "@angular/compiler": "^21.2.0",
    "@angular/core": "^21.2.0",
    "@angular/forms": "^21.2.0",
    "@angular/platform-browser": "^21.2.0",
    "@angular/platform-server": "^21.2.0",
    "@angular/router": "^21.2.0",
    "@angular/service-worker": "^21.2.0",
    "@angular/ssr": "^21.2.0",
    "@ngx-translate/core": "^16.0.4",
    "@ngx-translate/http-loader": "^16.0.1",
    "express": "^5.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular-eslint/builder": "^21.2.0",
    "@angular-eslint/eslint-plugin": "^21.2.0",
    "@angular-eslint/eslint-plugin-template": "^21.2.0",
    "@angular-eslint/template-parser": "^21.2.0",
    "@angular/build": "^21.2.0",
    "@angular/cli": "^21.2.0",
    "@angular/compiler-cli": "^21.2.0",
    "@cypress/schematic": "^5.0.0",
    "@eslint/js": "^9.39.3",
    "@types/express": "^5.0.1",
    "@types/node": "^20.17.19",
    "@typescript-eslint/eslint-plugin": "^8.56.1",
    "@typescript-eslint/parser": "^8.56.1",
    "@vitest/coverage-istanbul": "^4.0.18",
    "@vitest/coverage-v8": "^4.0.18",
    "cypress": "^15.11.0",
    "eslint": "^9.39.3",
    "jsdom": "^28.1.0",
    "openapi-typescript": "^7.12.0",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.56.1",
    "vitest": "^4.0.18",
    "zone.js": "^0.16.1"
  }
}
